{
  "name": "mdl-reactjs",
  "version": "0.1.0",
  "description": "React implementation of googles Material Design Lite (http://getmdl.io)",
  "main": "./lib",
  "keywords": [
    "React",
    "Material Design",
    "mdl",
    "material-design",
    "badge",
    "button",
    "card",
    "layout",
    "loading",
    "spinner",
    "menu",
    "slider",
    "toggles",
    "textfield",
    "fab-button",
    "icon",
    "icon-button",
    "switch",
    "progress",
    "progress bar",
    "tooltip",
    "dropdown",
    "grid",
    "tabs"
  ],
  "scripts": {
    "dev-build": "webpack --config webpack.config.js --colors",
    "dev-watch": "webpack --config webpack.config.js --watch --colors",
    "dev-server": "webpack-dev-server --config webpack.config.js --content-base ./docs/build/ --colors"
  },
  "author": "Johan Bostrom",
  "license": "GPL-3.0",
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "classnames": "^2.1.3",
    "lodash": "^3.10.1"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "html-loader": "^0.4.4",
    "markdown-loader": "^0.1.7",
    "react-router": "^3.0.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zarxor/mdl-reactjs.git"
  }
}
